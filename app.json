{
  "expo": {
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/SHOPDP2.png",
          "color": "#ffffff",
          "sounds": ["./assets/ringring.mp3", "./assets/ringring.mp3"],
          "mode": "production"
        }
      ]
    ],
    "name": "SHOP DP",
    "slug": "KLTN_MobileApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/SHOPDP2.png",
    "splash": {
      "image": "./assets/SHOPDP2.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/SHOPDP2.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.tdphuoc77.KLTN_MobileApp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
